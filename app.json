{
  "expo": {
    "version": "1.0.0",
    "name": "Pocket Dota",
    "description": "Dota 2 refference in you pocket",
    "slug": "pocket-dota",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "primaryColor": "#1B1E21",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B1E21"
    },
    "androidShowExponentNotificationInShellApp": false,
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "./src/assets/images/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "kriskate.pocketdota",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon-adaptive.png",
        "backgroundColor": "#1B1E21"
      }  
    }
  }
}
